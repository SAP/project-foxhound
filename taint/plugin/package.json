{
  "name": "taint-notifier",
  "version": "2.1.1",
  "description": "> A browser extension for Taintfox, that allows to examine websites for injection vulnerabilities",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:chrome": "npm run chrome-launch",
    "start:firefox": "npm run firefox-launch",
    "start:views": "node scripts/start.js",
    "dev": "npm run update-sources-and-sinks && node scripts/dev.js",
    "build": "npm run update-sources-and-sinks && node scripts/build.js",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ dist/ extension/ *.zip *.crx",
    "lint": "eslint ./ ",
    "lint:fix": "eslint ./ --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chrome-launch": "node scripts/dev.js --browser=chrome",
    "firefox-launch": "node scripts/dev.js --browser=firefox",
    "update-sources-and-sinks": "node scripts/update-sources-and-sinks.js"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "repository": {
    "type": "git",

include MIDITypes;

namespace mozilla {
namespace dom {

async protocol PMIDIManager
{
parent:
  async Refresh();
  async Shutdown();
child:
  /*
   * Send an updated list of MIDI ports to the child
   */
  async MIDIPortListUpdate(MIDIPortList aPortList);
};

} // namespace ipc
} // namespace mozilla

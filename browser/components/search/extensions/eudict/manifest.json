{
  "name": "EUdict Eng->Cro",
  "description": "EUdict - englesko-hrvatski rječnik",
  "manifest_version": 2,
  "version": "1.3",
  "browser_specific_settings": {
    "gecko": {
      "id": "eudict@search.mozilla.org"
    }
  },
  "hidden": true,
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "EUdict Eng->Cro",
      "search_url": "https://eudict.com",
      "search_url_get_params": "lang=engcro&word={searchTerms}"
    }
  }
}

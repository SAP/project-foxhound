{
  "name": "Yahoo! JAPAN",
  "description": "Yahoo Search",
  "manifest_version": 2,
  "version": "1.2",
  "browser_specific_settings": {
    "gecko": {
      "id": "yahoo-jp@search.mozilla.org"
    }
  },
  "hidden": true,
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Yahoo! JAPAN",
      "search_url": "https://search.yahoo.co.jp/search",
      "search_url_get_params": "p={searchTerms}&ei=UTF-8&fr=mozff"
    }
  }
}

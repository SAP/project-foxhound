  "title": "ASRouter event PingCentre ping",
  "type": "object",
  "properties": {
    "addon_version": {
      "type": "string"
    },
    "locale": {
      "type": "string"
    },
    "message_id": {
      "type": "string"
    },
    "event": {
      "type": "string"
    },
    "client_id": {
      "type": "string"
    },
    "impression_id": {
      "type": "string"
    }

SRCDIR=$(dirname $0)
TOPSRCDIR="$SRCDIR"/../..
PYTHON3="${PYTHON3:-python3}"
export OLD_CONFIGURE="$SRCDIR"/old-configure

set -- "$@" --enable-project=js

exec "$PYTHON3" "$TOPSRCDIR/configure.py" "$@"
